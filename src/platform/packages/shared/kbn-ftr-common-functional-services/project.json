{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/ftr-common-functional-services",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{workspaceRoot}/typings/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    },
    "jest": {
      "options": {
        "jestConfig": "{projectRoot}/jest.config.js"
      }
    },
    "lint": {}
  },
  "projectType": "library",
  "sourceRoot": "src/platform/packages/shared/kbn-ftr-common-functional-services"
}