{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/test",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.js",
      "{projectRoot}/src/jest/jest_flags.json",
      "!{projectRoot}/types/**/*",
      "!{projectRoot}/**/__fixtures__/**/*",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    },
    "jest": {
      "options": {
        "jestConfig": "{projectRoot}/jest.config.js"
      }
    },
    "lint": {}
  },
  "projectType": "library",
  "sourceRoot": "src/platform/packages/shared/kbn-test"
}