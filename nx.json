{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "cli": {
    "packageManager": "yarn"
  },
  "parallel": 5,
  "useDaemonProcess": true,
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": [
          "build",
          "test",
          "lint"
        ]
      }
    }
  },
  "plugins": [
  ],
  "namedInputs": {
    "global:tsConfigs": [
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.type_check.json",
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/tsconfig.base.type_check.json",
      "!{workspaceRoot}/nx.json",
      "!{workspaceRoot}/package.json",
      "!{workspaceRoot}/yarn.lock"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "executor": "@kbn/nx:typecheck-js",
      "dependsOn": [
        "^@kbn/nx",
        "^typecheck"
      ],
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ],
      "options": {
      },
      "cache": true
    },
  }
}
